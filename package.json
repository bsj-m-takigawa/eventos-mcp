{
  "name": "eventos-mcp",
  "version": "0.1.0",
  "description": "MCP server for eventos event management platform",
  "main": "dist/cli/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsx watch src/cli/server.ts",
    "start": "node dist/cli/server.js",
    "test": "jest",
    "test:contract": "jest tests/contract",
    "test:integration": "jest tests/integration",
    "test:unit": "jest tests/unit",
    "test:performance": "jest tests/performance",
    "test:mcp": "node test-mcp-server.cjs",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "cli": "tsx src/cli/server.ts"
  },
  "keywords": [
    "mcp",
    "eventos",
    "event-management",
    "ai-tools"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.5",
    "axios": "^1.11.0",
    "canvas": "^3.2.0",
    "dotenv": "^17.2.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.1.3",
    "msw": "^2.11.1",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
